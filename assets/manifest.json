{
  "manifest_version": 2,

  "name": "Todo",
  "description": "This extension helps you define task and update them once they are done",
  "version": "0.0.0.1",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" },
  "permissions": [
    "storage"
  ]
}
